    "module": "commonjs",
    "target": "ES2020",
    "lib": ["ES2020", "DOM"],
    "rootDir": ".",
    "outDir": "dist",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "types": ["node"]
